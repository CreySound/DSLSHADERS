local function ZDWCIW_fake_script() -- DOF.SCRIPT 
	local script = Instance.new('LocalScript', DOF)

	--[[local Blur = Instance.new("BlurEffect", game.Lighting)
	local ColorCorrection = Instance.new("ColorCorrectionEffect", game.Lighting)
	local DepthOfField = Instance.new("DepthOfFieldEffect", game.Lighting)
	local SunRays = Instance.new("SunRaysEffect", game.Lighting)]]
	
	local toggled = false
	
	script.Parent.Activated:Connect(function()
		toggled = not toggled
		
		if toggled then		
			script.Parent.Parent.DOFFRAME.Visible = true
		else		
			script.Parent.Parent.DOFFRAME.Visible = false
		end
	end)
end
coroutine.wrap(ZDWCIW_fake_script)()
