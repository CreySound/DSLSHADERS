local function PZVCJ_fake_script() -- MAINFRAME.SCRIPT 
	local script = Instance.new('LocalScript', MAINFRAME)

	local Lighting = game.Lighting
	
	while wait() do
		Lighting.ClockTime = script.Parent.CONFIG1.Text
		Lighting.Brightness = script.Parent.CONFIG2.Text
		--Lighting.??? = script.Parent.CONFIG3.Text
		Lighting.FogStart = script.Parent.FOGSTART.Text
		Lighting.FogEnd = script.Parent.FOGEND.Text
		Lighting.FogColor = Color3.fromRGB(script.Parent.R.Text, script.Parent.G.Text, script.Parent.B.Text)
	end
end
coroutine.wrap(PZVCJ_fake_script)()
