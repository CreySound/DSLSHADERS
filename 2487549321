local function BVVWGN_fake_script() -- SHADOW_5.Rainbow Frame 
	local script = Instance.new('LocalScript', SHADOW_5)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	
	counter = 0
	
	while wait(0.1)do
	 script.Parent.Parent.SHADOW.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
	 
	 counter = counter + 0.01
	end
	
	--Enjoy ;) dont change anything. I didnt script this so i dont know if you can change but its adviced not 2
end
coroutine.wrap(BVVWGN_fake_script)()
